
<files ./led-conf/connexion.php>
order allow,deny
deny from all
</files>
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R,L]
</IfModule>
Options +FollowSymLinks
RewriteEngine on

# Catch 404 errors
ErrorDocument 404 https://led-zepplin-forum.herokuapp.com/

